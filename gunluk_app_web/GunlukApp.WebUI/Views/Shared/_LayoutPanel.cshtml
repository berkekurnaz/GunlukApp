    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Günlük Sitesi Açıklama">
    <meta name="author" content="Berke Kurnaz">
    <meta name="keywords" content="Günlük Sitesi Açıklama">
    <!-- Title Page-->
    <title>Panel</title>
    <!-- Fontfaces CSS-->
    <link href="~/TemplatePanel/css/font-face.css" rel="stylesheet" />
    <link href="~/TemplatePanel/vendor/font-awesome-4.7/css/font-awesome.css" rel="stylesheet" />
    <link href="~/TemplatePanel/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" />
    <link href="~/TemplatePanel/vendor/mdi-font/css/material-design-iconic-font.css" rel="stylesheet" />
    <!-- Bootstrap CSS-->
    <link href="~/TemplatePanel/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" />
    <!-- Vendor CSS-->
    <link href="~/TemplatePanel/vendor/animsition/animsition.min.css" rel="stylesheet" />
    <link href="~/TemplatePanel/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" />
    <link href="~/TemplatePanel/vendor/wow/animate.css" rel="stylesheet" />
    <link href="~/TemplatePanel/vendor/css-hamburgers/hamburgers.css" rel="stylesheet" />
    <link href="vendor/slick/slick.css" rel="stylesheet" media="all">

    <link href="~/TemplatePanel/vendor/select2/select2.min.css" rel="stylesheet" />
    <link href="~/TemplatePanel/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" />
